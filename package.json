{
  "name": "web-routes",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/my-lib.js",
      "require": "./dist/my-lib.umd.cjs"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:code": "vitest --run",
    "test:code:watch": "vitest",
    "test:type": "tsd",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "clean-up": "npm run lint-and-fix & npm run pretty",
    "commit": "git-cz",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ilchenkoArtem/web-routes.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ilchenkoArtem/web-routes/issues"
  },
  "homepage": "https://github.com/ilchenkoArtem/web-routes#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "semantic-release": "^23.0.4",
    "tsd": "^0.30.7",
    "type-fest": "^4.12.0",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.4.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "tsd": {
    "directory": "src/nested-routes/tests-d"
  },
  "lint-staged": {
    "*": [
      "git add"
    ],
    "*.{ts}": [
      "prettier --write"
    ]
  },
  "files": [
    "index.d.ts",
    "dist"
  ]
}
